{
  "publisher": "tomjs",
  "name": "vscode-i18n",
  "displayName": "%displayName%",
  "version": "0.0.0",
  "private": true,
  "description": "%description%",
  "license": "MIT",
  "keywords": [
    "vscode"
  ],
  "categories": [
    "Other"
  ],
  "main": "dist/index.js",
  "icon": "resources/logo.png",
  "engines": {
    "node": ">=14",
    "vscode": "^1.75.0"
  },
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "tomjs.xxx.showHello",
        "title": "%tomjs.commands.hello%",
        "category": "%displayName%"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "%displayName%",
      "properties": {
        "tomjs.xxx.hello": {
          "type": "string",
          "default": "Hello World",
          "description": "%description%"
        }
      }
    }
  },
  "vsce": {
    "dependencies": false,
    "yarn": false
  },
  "scripts": {
    "dev": "run-p dev:*",
    "build": "tsdown && vscode-dev",
    "dev:lib": "tsdown --watch",
    "dev:i18n": "vscode-dev --watch"
  },
  "dependencies": {
    "@tomjs/vscode": "workspace:*"
  },
  "devDependencies": {
    "@tomjs/vscode-dev": "workspace:*",
    "@types/vscode": "^1.75.0"
  }
}
